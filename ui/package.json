{
  "name": "KryhaAgoricHackathon-ui",
  "version": "0.0.1",
  "description": "dapp-encouragement Frontend",
  "author": "Agoric",
  "license": "Apache-2.0",
  "homepage": ".",
  "scripts": {
    "build": "parcel build public/index.html",
    "lint-check": "eslint '**/*.{js,jsx}'",
    "lint-fix": "eslint --fix '**/*.{js,jsx}'",
    "parcel": "parcel serve --host=127.0.0.1 --port=3000 public/index.html",
    "start": "react-scripts start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "../.eslintrc.js"
    ]
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "public/lib",
        "staticOutDir": "lib"
      }
    ],
    "excludeGlob": "**/*.js",
    "watcherGlob": "**/*.html"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.1.0",
    "esm": "^3.2.25",
    "nodemon": "^2.0.3",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-icons": "^3.8.0",
    "react-iframe": "^1.8.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-select": "^3.0.8",
    "react-table": "^6.10.3",
    "styled-components": "^4.4.1",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2"
  }
}
